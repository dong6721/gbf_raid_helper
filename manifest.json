{
"manifest_version": 2,
"name": "reRaid",
"description": "Granblue fantasy raid search helper",
"version": "1.0",
"background": {
	"page": "background.html"
},
"icons": {
"128":"128.png"
},
"content_scripts": [
	{
	"js":["contentScript.js"],
	"matches":["<all_urls>"]
}
],
"commands": {
	"redirect_page": {
		"description":"redirect immediately"
	}
},
"permissions": [
	"clipboardRead",
	"clipboardWrite",
	"webRequest",
	"tabs",
	"fontSettings",
	"activeTab"
],
"browser_action": {
          "default_title": "test",      
          "default_popup": "popup.html"        
        }
}